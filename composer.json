{
  "name": "seothemes/child-theme-library",
  "type": "library",
  "description": "A lightweight drop-in library for extending Genesis child themes.",
  "version": "1.5.0",
  "keywords": [
    "wordpress",
    "wordpress-theme",
    "genesis-framework"
  ],
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "SEO Themes",
      "homepage": "https://seothemes.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/seothemes/child-theme-library/issues",
    "source": "https://github.com/seothemes/child-theme-library"
  },
  "require": {
    "php": "^5.4|^7"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
    "squizlabs/php_codesniffer": "^3.2.3",
    "wp-coding-standards/wpcs": "^0.14.1",
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^5.7",
    "brain/monkey": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "SEOThemes\\ChildThemeLibrary\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SEOThemes\\ChildThemeLibrary\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "kint-php/kint": "^2.2",
    "filp/whoops": "^2.1"
  },
  "config": {
    "sort-order": true
  },
  "scripts": {
    "phpcs": "phpcs",
    "test-unit": "phpunit --testsuite unit --colors",
    "test-integration": "phpunit --testsuite integration --colors",
    "test": [
      "@test-unit",
      "@test-integration"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
